# Start of puppet output for <%= realm %> 
<%= realm %>  = {
<% if default_domain.length > 0 -%>
default_domain = <%= default_domain %>
<% end -%>
<% if kpasswd_server.length > 0 -%>
kpasswd_server = <%= kpasswd_server %>
<% end -%>
<% if admin_server.length > 0 -%>
admin_server = <%= admin_server %>
 <% end -%>
<% if kdc.length > 0 -%>
<%    kdc.each do |s| -%>
kdc = <%= s %>
<%    end -%>
<% end -%>
<% if  v4_name_convert.length > 0 -%>
 v4_name_convert = {
<%  v4_name_convert.each do |a,b| -%>
     <%= a %> = {
<%   b.each do |i,j| -%>
        <%= i %> = <%= j %>
<%   end -%>
     }
<% end -%>
 }
<% end -%>
}


